<h2>Create New Contact </h2>

<%= form_for :contact, url: contacts_path  do |f| %>
    <p>
        
        <%= f.label :last %>
        <%= f.text_field :last %> 
    </p>
    
    
    <p>
         <%= f.label :first %> 
        <%= f.text_field :first %> 
        
    </p>
    
     <p>
         <%= f.label :company %> 
        <%= f.text_field :company %> 
        
    </p>
    
    <p>
         <%= f.label :address %>  
        <%= f.text_field :address %> 
        
    </p>
    
    <p>
         <%= f.label :email %>
        <%= f.text_field :email %> 
        
    </p>
    
       <p>
         <%= f.label :home %> 
        <%= f.text_field :home %> 
        
    </p>
    
      <p>
         <%= f.label :cell %> 
        <%= f.text_field :cell %> 
        
    </p>
      <p>
         <%= f.label :work %> 
        <%= f.text_field :work %> 
        
    </p>
    
    <p>
        <%= f.submit %>
    </p>
    
    <%end%>
    
    <%= link_to 'Back', contacts_path %>